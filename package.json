{
	"name": "playa",
	"version": "0.0.9",
	"description": "A musical theory framework aimed at creating musical phrases",
	"main": "build/cjs/index.js",
	"module": "build/esm/index.js",
	"scripts": {
		"test": "npm run lint && jest",
		"test:watch": "jest --watch",
		"test:update": "jest --updateSnapshot",
		"test:coverage": "jest --coverage",
		"check:comments": "leasot -x lib/**/*.js",
		"lint": "eslint lib test",
		"build": "rollup -c rollup.config.prod.js",
		"build:types": "tsc --project tsconfig.types.json",
		"start": "rollup -c rollup.config.dev.js --watch",
		"prepare": "npm run build",
		"docs:build": "rollup -c rollup.config.docs.js && docma -c docma.json --clean",
		"docs:serve": "docma serve"
	},
	"author": {
		"name": "Ricardo Matias",
		"url": "https://github.com/ricardomatias"
	},
	"homepage": "https://ricardomatias.net/playa",
	"repository": {
		"type": "git",
		"url": "git://github.com/ricardomatias/playa.git"
	},
	"bugs": {
		"url": "https://github.com/ricardomatias/playa/issues"
	},
	"license": "OSL 3.0",
	"husky": {
		"hooks": {
			"pre-push": "npm test",
			"pre-commit": "npm run docs:build"
		}
	},
	"keywords": [
		"music",
		"music theory",
		"audio",
		"web audio",
		"sound",
		"midi",
		"playa"
	],
	"files": [
		"build/*"
	],
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-typescript": "^7.8.3",
		"@otris/jsdoc-tsd": "^2.0.0",
		"@rollup/plugin-typescript": "^3.0.0",
		"@types/jest": "^25.1.3",
		"@types/ramda": "^0.26.42",
		"@typescript-eslint/eslint-plugin": "^2.20.0",
		"@typescript-eslint/parser": "^2.20.0",
		"babel-jest": "^25.1.0",
		"docma": "^3.2.2",
		"eslint": "^6.8.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-import": "^2.20.1",
		"esm": "^3.2.25",
		"husky": "^1.2.0",
		"jest": "^25.1.0",
		"jsdoc-memberof-namespace": "^2.2.0",
		"leasot": "^10.1.0",
		"mocha": "^7.0.1",
		"prettier": "^1.19.1",
		"rollup": "^1.31.1",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-cleanup": "^3.1.1",
		"rollup-plugin-clear": "^2.0.7",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-delete": "^1.2.0",
		"rollup-plugin-gzip": "^2.3.0",
		"rollup-plugin-includepaths": "^0.2.3",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-progress": "^1.1.1",
		"rollup-plugin-terser": "^5.2.0",
		"rollup-plugin-typescript2": "^0.26.0",
		"tsd-jsdoc": "^2.4.0",
		"typescript": "^3.8.2"
	},
	"dependencies": {
		"@ricardomatias/ring": "0.0.4",
		"@ricardomatias/roll": "^0.1.0",
		"alea": "^1.0.0",
		"ramda": "^0.27.0",
		"roman-numeral": "^0.2.6",
		"simplex-noise": "^2.4.0"
	}
}
