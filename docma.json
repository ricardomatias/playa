{
	"debug": 5,
	"src": [
		"./build/esm/index.js",
		"./lib/types.js",
		"./README.md"
	],
	"jsdoc": {
		"source": {
			"includePattern": ".+\\.(j|t)s(doc|x)?$"
		},
		"encoding": "utf8",
		"recurse": true,
		"pedantic": false,
		"access": null,
		"package": null,
		"module": true,
		"undocumented": false,
		"undescribed": false,
		"ignored": false,
		"hierarchy": true,
		"sort": "access",
		"relativePath": null,
		"filter": null,
		"allowUnknownTags": true,
		"opts": {
			"template": "./node_modules/tsd-jsdoc/dist"
		}
	},
	"markdown": {
		"gfm": true,
		"tables": true,
		"breaks": false,
		"pedantic": false,
		"sanitize": false,
		"smartLists": true,
		"smartypants": false,
		"xhtml": false,
		"tasks": true,
		"emoji": true
	},
	"app": {
		"title": "PLAYA",
		"routing": "query",
		"entrance": "content:readme",
		"base": "/playa",
		"favicon": "assets/favicon.ico",
		"server": "github"
	},
	"template": {
		"path": "zebra",
		"options": {
			"title": null,
			"logo": {
				"dark": "assets/logo.png",
				"light": "assets/logo-transparent.png"
			},
			"sidebar": {
				"enabled": true,
				"outline": "tree",
				"collapsed": false,
				"toolbar": true,
				"itemsFolded": true,
				"itemsOverflow": "crop",
				"badges": true,
				"search": true,
				"animations": true
			},
			"symbols": {
				"autoLink": true,
				"params": "table",
				"enums": "table",
				"props": "table",
				"meta": false
			},
			"contentView": {
				"bookmarks": "h1,h2,h3",
				"faLibs": "solid,regular,brands"
			},
			"navbar": {
				"enabled": true,
				"dark": true,
				"animations": true,
				"menu": [
					{
						"label": "Readme",
						"href": "/playa"
					},
					{
						"label": "Documentation",
						"href": "?api"
					},
					{
						"label": "GitHub",
						"iconClass": "fab fa-github",
						"href": "https://github.com/ricardomatias/playa",
						"target": "_blank"
					}
				]
			}
		}
	},
	"dest": "./docs",
	"assets": {
		"/assets": [
			"./assets/logo.png",
			"./assets/logo-transparent.png",
			"./assets/favicon.ico"
		]
	},
	"clean": true
}
