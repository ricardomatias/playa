{
	"debug": 5,
	"src": [
		"./lib/core/*.js",
		"./lib/functional/**/*.js",
		"!./lib/functional/*.bk.js",
		"./lib/tools/*.js",
		"./lib/utils/*.js",
		"./lib/constants/*.js",
		"./README.md",
		{
			"Core": "./lib/core/*.js",
			"Functional": ["./lib/functional/*.js", "!./lib/functional/*.bk.js"],
			"Tools": "./lib/tools/*.js",
			"Utils": "./lib/utils/*.js"
		}
	],
	"jsdoc": {
		"encoding": "utf8",
		"recurse": true,
		"pedantic": false,
		"access": null,
		"package": null,
		"module": true,
		"undocumented": false,
		"undescribed": false,
		"ignored": false,
		"hierarchy": true,
		"sort": "access",
		"relativePath": null,
		"filter": null,
		"allowUnknownTags": true,
		"plugins": [],
		"opts": {
			"template": "./node_modules/tsd-jsdoc/dist"
		}
	},
	"markdown": {
		"gfm": true,
		"tables": true,
		"breaks": false,
		"pedantic": false,
		"sanitize": false,
		"smartLists": true,
		"smartypants": false,
		"xhtml": false,
		"tasks": true,
		"emoji": true
	},
	"app": {
		"title": "PLAYA",
		"routing": "query",
		"entrance": "content:readme",
		"base": "/",
		"favicon": "assets/favicon.ico"
	},
	"template": {
		"path": "zebra",
		"options": {
			"title": null,
			"logo": {
				"dark": "assets/logo.png",
				"light": "assets/logo-transparent.png"
			},
			"sidebar": {
				"enabled": true,
				"outline": "tree",
				"collapsed": false,
				"toolbar": true,
				"itemsFolded": true,
				"itemsOverflow": "crop",
				"badges": true,
				"search": true,
				"animations": true
			},
			"symbols": {
				"autoLink": true,
				"params": "table",
				"enums": "table",
				"props": "table",
				"meta": false
			},
			"contentView": {
				"bookmarks": "h1,h2,h3",
				"faLibs": "solid,regular,brands"
			},
			"navbar": {
				"enabled": true,
				"dark": true,
				"animations": true,
				"menu": [
					{
						"label": "Readme",
						"href": "/"
					},
					{
						"label": "Documentation",
						"href": "?api"
					}
				]
			}
		}
	},
	"dest": "./docs",
	"assets": {
		"/assets": [
			"./assets/logo.png",
			"./assets/logo-transparent.png",
			"./assets/favicon.ico"
		]
	},
	"clean": true
}
